#!/bin/sh

# generates a startup.pl to be run by apache at startup
# you should run this in the directory where the source code is in.
# you must run this everytime you change the source code path

pwd=`pwd`
libd="$pwd/lib"

cat <<END
#!/usr/bin/perl
# AUTOGENERATED FILE, don't edit
# this was autogenerated with ./gen_startup.pl
use lib qw($libd);
use Semece;

1;
END
